{
    "name": "7lab/laravel-or-abort",
    "description": "Same as findOrFail/firstOrFail without throwing an exception.",
    "keywords": [
        "7lab",
        "laravel-or-abort",
        "laravel",
        "find-or-fail",
        "first-or-fail",
        "abort",
        "404"
    ],
    "homepage": "https://github.com/7lab/laravel-or-abort",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjan Kempes",
            "email": "arjan@7lab.nl",
            "homepage": "https://www.7lab.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "SevenLab\\OrAbort\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SevenLab\\OrAbort\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}